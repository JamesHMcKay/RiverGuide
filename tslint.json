{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "object-literal-sort-keys": false,
        "no-shadowed-variable": false
    },
    "rules": {
        "object-literal-sort-keys": false,
        "no-shadowed-variable": false,
        "no-console": [true, "log"]
    },
    "rulesDirectory": [],
    "tslint.jsEnable": false,
    "linterOptions": {
        "exclude": [
            "bin",
            "*/*.js",
            "*/*/*.js"
        ]
    }
}